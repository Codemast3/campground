<% layout('layouts/boilerplate') %>
<div id="map" style="width: 100%; height: 500px"></div>
    <h1>
        All Campground
    </h1>
    <div>
      <a href="/campgrounds/new">add new campground</a>
    </div>
    
    
    
      <% for(let campground of campgrounds) {
        %>
       <div class="card" mb-3>
        <div class="row">
          <div class="col-md-4">
            <% if(campground.images.length) { %>
            <img src="https://res.cloudinary.com/dqtrikkhp/image/upload/v1737322516/Campground/iumqx4t9aebsdqvvts3z.png" class="img-fluid" alt="<%=campground.title%>">
            <% } %>
          </div>
          <div class="col-md-8">
            <div class="card-body">
              <h5 class="card-title"><%=campground.title%></h5>
              <p class="card-text"><%=campground.location%></p>
              <a href="/campgrounds/<%=campground._id%>" class="btn btn-primary">View</a>
            </div>
          </div>
        </div>
       </div>
        <% } %>


        <script>
          const maptilerApiKey = '<%- process.env.MAPTILER_API_KEY %>';
          const campgrounds = { features: <%- JSON.stringify(campgrounds) %>}
      </script>

      
